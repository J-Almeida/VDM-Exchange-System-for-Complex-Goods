--stores the products in stock
class Stock
instance variables
	public products: set of Product := {};
operations
	public addProduct: Product ==> () --adds a product to stock
		addProduct(p) == (
			products := products union {p}
		)
		post p in set products;
	
	public removeProduct: Product ==> () --removes a product from stock
		removeProduct(p) == (
			products := products \ {p}
		)
		pre p in set products
		post p not in set products;
	
functions

end Stock